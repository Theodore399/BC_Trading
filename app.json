{
  "expo": {
    "name": "BC_Trading",
    "slug": "BC_Trading",
    "scheme": ["trading"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/logo.jpg"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
